<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kieutrang.e_learning">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".E_Learning">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name="com.example.kieutrang.e_learning.vocabulary"></activity>
        <activity android:name="com.example.kieutrang.e_learning.exercise"></activity>
        <activity android:name="com.example.kieutrang.e_learning.test_english"></activity>
        <activity android:name="com.example.kieutrang.e_learning.extra"></activity>
        <activity android:name=".test_offline"></activity>
        <activity android:name="com.example.kieutrang.e_learning.testoff_lesson"></activity>
        <activity android:name="com.example.kieutrang.e_learning.vol_lesson"></activity>
        <activity android:name="com.example.kieutrang.e_learning.extra_lesson"></activity>
        <activity android:name="com.example.kieutrang.e_learning.test_online"></activity>
    </application>

</manifest>
